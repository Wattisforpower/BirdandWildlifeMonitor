  HAL_GPIO_WritePin(GPIOA, LED_STATUS_1_Pin|LED_STATUS_3_Pin, GPIO_PIN_RESET);

  /*Configure GPIO pin Output Level */
  HAL_GPIO_WritePin(LED_STATUS_2_GPIO_Port, LED_STATUS_2_Pin, GPIO_PIN_RESET);

